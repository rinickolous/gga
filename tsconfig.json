{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "types": ["@league-of-foundry-developers/foundry-vtt-types","node"],
    "typeRoots": ["../node_modules/@types"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "sourceMap": true,
    "strictNullChecks": false
  },
  "include": ["src"]
}
